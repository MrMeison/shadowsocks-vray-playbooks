{
    "server":"0.0.0.0",
    "server_port":9000,
    "password":"{{ proxy_password }}",
    "timeout":120,
    "method":"{{ method }}",
    "fast_open": {{ "true" if fast_open else "false" }},
    "no_delay": true,
    "reuse_port": true,
    "nameserver":"1.1.1.1",
    "mode":"tcp_only",
    "plugin":"v2ray-plugin",
    "plugin_opts":"server;tls;host={{ host }};cert=/data/certs/le-crt.pem;key=/data/certs/le-key.pem;loglevel=none;",
    "log": {
        "config_path": "/etc/shadowsocks-rust/log4rs.yml"
    }
}