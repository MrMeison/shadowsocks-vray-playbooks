{
    "server":"{{ host }}",
    "server_port":443,
    "local_port":{{ local_port }},
    "mode":"tcp_only",
    "password":"{{ proxy_password }}",
    "timeout":300,
    "method": "{{ method }}",
    "fast_open": {{ "true" if fast_open else "false" }},
    "reuse_port": true,
    "nameserver": "adguard",
    "plugin":"v2ray-plugin",
    "plugin_opts":"tls;path=/{{ endpoint }};host={{ host }}"
} 
